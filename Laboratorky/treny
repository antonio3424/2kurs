Capitals = dict()  # создаем пустой словарь

# Заполняем словарь значениями
Capitals['Карась', 'карась', 'карасик', 'караси', 'Карасик', 'Караси'] = 'ссылка'
Capitals['Карась золотой', 'карась золотой', 'Карась Золотой', 'карась Золотой', 'Карась золотистый', 'карасик золотой',
         'Карасик золотистый'] = 'ссылка'
Capitals['Карась Серебряный', 'карась Серебряный', 'Карась серебряный', 'карась серебряный', 'карасик Серебряный',
         'Карасик серебряный', 'Карасик серебряный'] = 'ссылка'
Capitals['Карп', 'Карпик', 'карп', 'карпик', 'Карпёнок', 'карпёнок'] = 'ссылка'
Capitals['Карп Зеркальный', 'Карп зеркальный', 'карп зеркальный', 'Карпик Зеркальный', 'карпик зеркальный'] = 'ссылка'
Capitals['Лещ', 'лещ', 'Лещик', 'лещик'] = 'ссылка'
Capitals['Подлещик', 'П', ''] = 'ссылка'
Capitals['', '', ''] = 'ссылка'

Fishes = ['', '', '', '', '', '', '', '', '', '', ]

for fish in Fishes:
    # для каждой рыбы из списка проверим,есть ли она в словаре
    if fish in Capitals:
        print('Название рыбы' +fish + ':' + Capitals[fish])  # выдает данные
    else:
        print('В базе нет такой рыбы, и информации по поводу данной рыбы ' + fish)
